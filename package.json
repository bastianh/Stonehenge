{
  "name": "@resir014/stonehenge",
  "version": "0.0.0",
  "description": "Next-generation AI scripts for the game Screeps. Written in TypeScript.",
  "main": "./dist/main.js",
  "scripts": {
    "commit": "git-cz",
    "gulp": "gulp",
    "postinstall": "typings install",
    "deploy": "gulp build",
    "deploy-prod": "gulp build --target prod",
    "deploy-local": "gulp build --target local",
    "start": "gulp watch",
    "prod": "gulp watch --target prod",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "tslint": "tslint \"src/**/*.ts\" \"test/**/*.ts\" --exclude \"src/lib/**/*.ts\"",
    "typings": "typings",
    "jshint-gulp": "jshint gulpfile.js libs/gulp-screeps-upload.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resir014/Stonehenge.git"
  },
  "author": "Resi Respati <resir014@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resir014/Stonehenge/issues"
  },
  "homepage": "https://github.com/resir014/Stonehenge#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/sinon": "^2.1.2",
    "chai": "^3.5.0",
    "deep-equal": "^1.0.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-clean": "^0.3.2",
    "gulp-collect": "^0.1.0",
    "gulp-dot-flatten": "^1.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.6.3",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-tslint": "^6.0.1",
    "gulp-typescript": "^3.0.0",
    "gulp-util": "^3.0.7",
    "istanbul": "^1.1.0-alpha.1",
    "jshint": "^2.9.2",
    "lodash": "4.17.19",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "q": "^1.4.1",
    "recast": "^0.11.10",
    "simple-git": "^1.61.0",
    "sinon": "^2.1.0",
    "source-map": "^0.5.6",
    "source-map-loader": "^0.1.6",
    "through2": "^2.0.1",
    "ts-loader": "^2.0.0",
    "ts-node": "^3.0.2",
    "tslint": "^3.13.0",
    "typescript": "^2.0.3",
    "typings": "^1.3.0",
    "webpack": "^2.2.1",
    "webpack-stream": "^3.2.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
